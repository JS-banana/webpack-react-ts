{
  "name": "webpack-react-ts",
  "version": "0.0.1",
  "description": "a webpack5.x + react + typescript practices",
  "author": "JS-banana",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JS-banana/webpack-react-ts"
  },
  "bugs": {
    "url": "https://github.com/JS-banana/webpack-react-ts/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "npm run dev",
    "dev": "webpack serve --progress --config config/webpack.dev.js --mode development",
    "build": "webpack --config config/webpack.prod.js --mode production",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf ./dist",
    "prettify": "prettier --write",
    "pretty-quick": "pretty-quick",
    "lint:eslint:fix": "eslint --ext .ts,.tsx --no-error-on-unmatched-pattern --quiet --fix",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint": "npm run lint:eslint:fix && npm run lint:style",
    "commitlint": "commitlint --from=master",
    "serve": "serve --no-clipboard --single --listen 3000 dist",
    "test": "jest",
    "test-c": "jest --coverage",
    "test:clean": "rimraf ./coverage"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "browserslist": [
    "last 1 versions",
    "> 2%",
    "not dead"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "pretty-quick --staged",
      "git add --force"
    ]
  },
  "dependencies": {
    "antd": "^4.16.10",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "core-js": "3",
    "dayjs": "^1.10.6",
    "history": "^5.0.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/plugin-transform-react-constant-elements": "^7.14.5",
    "@babel/plugin-transform-react-inline-elements": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.4",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "compression-webpack-plugin": "^8.0.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-redux-saga": "^1.2.1",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react-refresh": "^0.10.0",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^22.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.1.4",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.49.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-pwa-manifest": "^4.3.0",
    "yargs": "^17.1.0"
  },
  "resolutions": {
    "babel-loader": "8.2.2"
  }
}
